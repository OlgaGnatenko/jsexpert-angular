{
    "extends": "../tslint.json",
    "rules": {
        "no-inferrable-types": false,
        "max-line-length": false,
        "quotemark": false,
        "no-trailing-whitespace": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            false,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
